{
  "presets": [
    [
      "@babel/preset-env",
      {
        "debug": true,
        "useBuiltIns": "usage",
        "loose": true,
        "targets": {
          "node": true,
          "browsers": [
            "> 1%",
            "not ie <= 8",
            "last 2 versions",
            "iOS >= 7",
            "Android >= 5"
          ]
        },
        "modules": false
      }
    ],
    "@babel/preset-react",
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ],
    "@babel/plugin-transform-object-assign",
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
  ]
}